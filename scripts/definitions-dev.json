{
    "vhosts": [
        {
            "name": "keywords-dev"
        }
    ],
    "users": [
        {
            "name": "read-content-dev",
            "password": "dev123",
            "tags": ""
        },
        {
            "name": "extract-dev",
            "password": "dev123",
            "tags": ""
        },
        {
            "name": "dev-admin",
            "password": "admin123",
            "tags": "administrator"
        }
    ],
    "permissions": [
        {
            "user": "read-content-dev",
            "vhost": "keywords-dev",
            "configure": "^$",
            "write": "^qchunks_ex$",
            "read": "^$"
        },
        {
            "user": "extract-dev",
            "vhost": "keywords-dev",
            "configure": "^$",
            "write": "^$",
            "read": "^qchunks(\\.dlq)?$"
        },
        {
            "user": "dev-admin",
            "vhost": "keywords-dev",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        }
    ],
    "exchanges": [
        {
            "name": "qchunks_ex",
            "vhost": "keywords-dev",
            "type": "direct",
            "durable": true,
            "auto_delete": false,
            "internal": false,
            "arguments": {}
        },
        {
            "name": "qchunks_ex.dlq",
            "vhost": "keywords-dev",
            "type": "direct",
            "durable": true,
            "auto_delete": false,
            "internal": false,
            "arguments": {}
        }
    ],
    "queues": [
        {
            "name": "qchunks",
            "vhost": "keywords-dev",
            "durable": true,
            "auto_delete": false,
            "arguments": {
                "x-queue-type": "classic",
                "x-message-ttl": 600000,
                "x-dead-letter-exchange": "qchunks_ex.dlq",
                "x-dead-letter-routing-key": "message.qchunks.dlq"
            }
        },
        {
            "name": "qchunks.dlq",
            "vhost": "keywords-dev",
            "durable": true,
            "auto_delete": false,
            "arguments": {
                "x-queue-type": "classic"
            }
        }
    ],
    "bindings": [
        {
            "source": "qchunks_ex",
            "vhost": "keywords-dev",
            "destination": "qchunks",
            "destination_type": "queue",
            "routing_key": "message.qchunks",
            "arguments": {}
        },
        {
            "source": "qchunks_ex.dlq",
            "vhost": "keywords-dev",
            "destination": "qchunks.dlq",
            "destination_type": "queue",
            "routing_key": "message.qchunks.dlq",
            "arguments": {}
        }
    ],
    "parameters": [],
    "policies": []
}